<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>createUserServlet</servlet-name>
		<display-name>createUserServlet</display-name>
		<description></description>
		<servlet-class>com.murali.user.servlets.createUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DisplayUserDetailsServlet</servlet-name>
		<display-name>DisplayUserDetailsServlet</display-name>
		<description></description>
		<servlet-class>com.murali.user.servlets.DisplayUserDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createUserServlet</servlet-name>
		<url-pattern>/createUserServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DisplayUserDetailsServlet</servlet-name>
		<url-pattern>/DisplayUserDetailsServlet</url-pattern>
	</servlet-mapping>
</web-app>
